{
  "name": "ncms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:migrate": "dotenv -e .env.prisma.local -- npx prisma migrate deploy",
    "prisma:seed": "dotenv -e .env.prisma.local -- npx prisma db seed",
    "dockerfile-cmd:dev": "npm run prisma:migrate && npm run prisma:seed && npm run dev",
    "dockerfile-cmd:prod": "npm run prisma:migrate && npm run prisma:seed && node server.js",
    "docker-compose-up:dev": "docker compose -f docker-compose.yaml --profile dev up -d",
    "docker-compose-down:dev": "docker compose -f docker-compose.yaml --profile dev down",
    "docker-compose-up:prod": "docker compose -f docker-compose.yaml --profile prod up -d",
    "docker-compose-down:prod": "docker compose -f docker-compose.yaml --profile dev down"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.2",
    "@prisma/client": "^5.18.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.424.0",
    "mapbox-gl": "^3.5.2",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.20",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.18.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
